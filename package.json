{
	"name": "server-boilerplate",
	"productName": "server-boilerplate",
	"version": "1.0.0",
	"description": "Multiplayer Sketchbook",
	"main": ".webpack/main",
	"scripts": {
		"setup": "npm install && npm fund && npm run build",
		"build:client": "webpack --config ./src/webpack.prod.client.ts",
		"build:server": "webpack --config ./src/webpack.prod.server.ts",
		"build:turbo": "turbo run build_struct",
		"build": "concurrently -k \"npm run build:client && npm run build:server\"",
		"dev": "concurrently -k \"webpack serve --config ./src/webpack.dev.ts\"",
		"dev:client": "webpack serve --config ./src/webpack.dev.client.ts",
		"dev:server": "webpack serve --config ./src/webpack.dev.server.ts",
		"dev:server:start": "nodemon --watch dist ./dist/server/server.js",
		"start:server": "node ./dist/server/server.js",
		"fcm": "node forgeConfigMaker.js",
		"start:nw": "npm run fcm && electron-forge start",
		"start": "electron-forge start",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"publish": "electron-forge publish",
		"lint": "eslint --ext .ts,.tsx .",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [],
	"author": {
		"name": "tkkaushik369",
		"email": "krishnakaushiktiruveedula369@gmail.com"
	},
	"license": "MIT",
	"packageManager": "npm@8.5.0",
	"devDependencies": {
		"@electron-forge/cli": "^7.5.0",
		"@electron-forge/maker-deb": "^7.5.0",
		"@electron-forge/maker-rpm": "^7.5.0",
		"@electron-forge/maker-squirrel": "^7.5.0",
		"@electron-forge/maker-zip": "^7.5.0",
		"@electron-forge/plugin-auto-unpack-natives": "^7.5.0",
		"@electron-forge/plugin-fuses": "^7.5.0",
		"@electron-forge/plugin-webpack": "^7.5.0",
		"@electron/asar": "^3.2.17",
		"@electron/fuses": "^1.8.0",
		"@types/debug": "^4.1.12",
		"@types/electron-squirrel-startup": "^1.0.2",
		"@types/express": "^4.17.21",
		"@types/node": "^22.18.0",
		"@typescript-eslint/eslint-plugin": "^8.14.0",
		"@typescript-eslint/parser": "^8.14.0",
		"@vercel/webpack-asset-relocator-loader": "1.7.3",
		"concurrently": "^9.0.1",
		"copy-webpack-plugin": "^12.0.2",
		"css-loader": "^7.1.2",
		"electron": "^33.1.0",
		"eslint": "^9.14.0",
		"eslint-plugin-import": "^2.31.0",
		"fork-ts-checker-webpack-plugin": "^9.0.2",
		"html-webpack-plugin": "^5.6.3",
		"node-loader": "^2.1.0",
		"nodemon": "^3.1.10",
		"style-loader": "^4.0.0",
		"ts-loader": "^9.5.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.6.3",
		"webpack": "^5.95.0",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^5.1.0",
		"webpack-merge": "^6.0.1"
	},
	"dependencies": {
		"electron-squirrel-startup": "^1.0.1",
		"express": "^4.21.1",
		"turbo": "^2.5.6"
	},
	"optionalDependencies": {
		"bufferutil": "^4.0.8",
		"utf-8-validate": "^5.0.10"
	}
}
